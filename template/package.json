    "@rematch/loading": "1.2.1",
    "@uiw/formatter": "1.2.4",
    "@uiw/react-native": "1.7.4",
    "react": "16.13.1",
    "react-native": "0.63.2",
    "react-native-device-info": "5.6.4",
